{
  "expo": {
    "name": "Beam",
    "slug": "Beam",
    "version": "1.2.6",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2A2C36"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abdelhady154.beam",
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "Beam Store Uses the Gallery to let you change your profile Image"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#2A2C36"
      },
      "package": "com.abdelhady154.beam",
      "versionCode": 16
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#ffffff",
          "mode": "production"
        }
      ]
    ]
  }
}
